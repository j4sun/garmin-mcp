{
  "name": "garmin-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Garmin Connect integration with activity insights and training suggestions",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "RUN_INTEGRATION_TESTS=true jest test/integration.test.ts",
    "test:unit": "jest --testPathIgnorePatterns=integration.test.ts"
  },
  "keywords": [
    "mcp",
    "garmin",
    "fitness",
    "training",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "dotenv": "^16.3.1",
    "garmin-connect": "^1.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "tsx": "^4.0.0",
    "typescript": "^5.9.2"
  }
}
